
<%= form_for :song, remote: true do |f| %>
<% if @song.errors.any? %>
  <h2>Errors: </h2>
   <ul>
<% @song.errors.full_messages.each do |message|%>
<div class="error-wrapper">
  <%= message %>
</div>
<% end %>
  </ul>
<% end %>

<%= f.label :title %> <br />
<%= f.text_field :title %> <br />

<%= f.label :genre %> <br />
<%= f.text_field :genre %> <br /><br />
<%= f.submit 'Add Song', class: "btn btn-default", id:"addsong" %>

<% end %>
